public class Main {
    public static void main(String[] args) {

        Cliente cliente = new Cliente();
        cliente.nombre= "Antonio";
        cliente.edad= 35;
        cliente.telefono= 123456789;
        
        System.out.println("Mi nombre es " + cliente.nombre + " mi edad es " + cliente.edad + " y mi telefono es " + cliente.telefono);
    }
}


abstract class Persona{
    int edad;
    String nombre;
    int telefono;
}


class Cliente extends Persona {
    int credito;
}


class Trabajador extends Persona{
    String salario;
}

